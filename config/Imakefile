#include "NetAudio.tmpl"

INSTALLFLAGS = $(INSTINCFLAGS)

HEADERS = NasConfig.h config.h

config.h: 
	sh -c "unset CFLAGS; ./configure"

includes:: config.h

all::
depend::


clean::
	rm -f config.h config.cache config.status

